<launch>
	
	<!-- Start the novatel node -->
	<node pkg="novatel" type="novatel_node" name="novatel_right" output="log" 
	    required="true">
		<param name="port" value="/dev/ttyUSB1" />
		<param name="baudrate" value="115200" />

	    <param name="log_ephemeris" value="false" />

		<param name="log_commands" value="BESTUTMB ONTIME 0.5; BESTVELB ONTIME 0.5; RANGECMPB ONTIME 0.5; BESTXYZB ONTIME 0.5" />
		<param name="gps_default_logs_period" value="0.5" />
		
	</node>

</launch>
